  object q_pesquisa_cod: TFDQuery
    Connection = dm.conexao
    SQL.Strings = (
      
        'SELECT A.COD_PRODUTO, A.DESCRICAO, A.PRECO_VENDA, A.CATEGORIA_DE' +
        'SC, A.CATEGORIA, B.COD_BARRAS, C.DESCRICAO AS TAMANHO FROM PRODU' +
        'TOS A, PRODUTOS_BARRA B, TAMANHOS C '
      
        'WHERE A.COD_PRODUTO = B.COD_PRODUTO AND B.TAMANHO = C.CODIGO AND' +
        ' A.COD_PRODUTO = :cod_produto'
      '')
    Left = 168
    Top = 400
    ParamData = <
      item
        Name = 'COD_PRODUTO'
        ParamType = ptInput
      end>
    object q_pesquisa_codCOD_PRODUTO: TFDAutoIncField
      FieldName = 'COD_PRODUTO'
      Origin = 'COD_PRODUTO'
      ProviderFlags = [pfInWhere, pfInKey]
      ReadOnly = True
    end
    object q_pesquisa_codDESCRICAO: TWideStringField
      FieldName = 'DESCRICAO'
      Origin = 'DESCRICAO'
      Size = 50
