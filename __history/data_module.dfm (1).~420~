      end>
    SQL.Strings = (
      'SELECT SUM(VALOR) AS VALOR, DATA_QUITACAO FROM'#11
      ''
      #11
      ''
      
        '(SELECT SUM(A.VALOR_PRINCIPAL) AS VALOR, TRUNC(A.DATA_BAIXA) AS ' +
        'DATA_QUITACAO'#11
      ''
      ' FROM BAIXA_CONTA_PAGAR A, CONTA_PAGAR B '#11
      ''
      
