  object q_consulta: TADOQuery
    AutoCalcFields = False
    CursorType = ctStatic
    Parameters = <
      item
        Name = 'data1'
        Attributes = [paNullable]
        DataType = ftString
        Precision = 231
        Size = 999
        Value = Null
      end
      item
        Name = 'data2'
        Attributes = [paNullable]
        DataType = ftString
        Precision = 231
        Size = 999
        Value = Null
      end>
    SQL.Strings = (
      
        'SELECT A.CONTA_DEBITO, A.CONTA_CREDITO, A.VALOR, A.CCUSTO_DEBITO' +
        ', A.CCUSTO_CREDITO, A.DATA FROM LANCAMENTO_CONTABIL A'
      
        'WHERE A.DATA >= :data1 and A.DATA <= :data2 AND A.COD_GRUPO_PC =' +
        ' 25 AND (A.CCUSTO_DEBITO <> 0 OR A.CCUSTO_CREDITO <> 0 )')
    Left = 64
    Top = 264
    object q_consultaCONTA_DEBITO: TStringField
      FieldName = 'CONTA_DEBITO'
    end
    object q_consultaCONTA_CREDITO: TStringField
      FieldName = 'CONTA_CREDITO'
    end
    object q_consultaVALOR: TBCDField
      FieldName = 'VALOR'
      Precision = 15
      Size = 2
    end
    object q_consultaCCUSTO_DEBITO: TIntegerField
      FieldName = 'CCUSTO_DEBITO'
    end
    object q_consultaCCUSTO_CREDITO: TIntegerField
      FieldName = 'CCUSTO_CREDITO'
    end
    object q_consultaDATA: TDateTimeField
      FieldName = 'DATA'
    end
  end
  object q_consulta2: TADOQuery
    Connection = dm.conexao
    Parameters = <
      item
        Name = 'data1'
        Attributes = [paNullable]
        DataType = ftWideString
        Precision = 231
        Size = 999
        Value = Null
      end
      item
        Name = 'data2'
        Attributes = [paNullable]
        DataType = ftWideString
        Precision = 231
        Size = 999
        Value = Null
      end>
    SQL.Strings = (
      
        'SELECT A.CONTA_DEBITO, A.CONTA_CREDITO, A.VALOR, A.CCUSTO_DEBITO' +
        ', A.CCUSTO_CREDITO, A.DATA, B.COD_FABRICA FROM LANCAMENTO_CONTAB' +
        'IL A, CONTA_CONTABIL B '#11
      ''
      
        'WHERE A.DATA >= :data1 and A.DATA <= :data2 AND A.COD_GRUPO_PC =' +
        ' 25 AND (A.CCUSTO_DEBITO <> 0 OR A.CCUSTO_CREDITO <> 0) AND'#11
      ''
      
        '(A.CONTA_DEBITO = B.COD_CONTABIL OR A.CONTA_CREDITO = B.COD_CONT' +
        'ABIL) AND B.COD_FABRICA <> 0 AND A.COD_GRUPO_PC = B.COD_GRUPO_PC')
    Left = 64
    Top = 328
  end
